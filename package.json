  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-plugin-jest": "^24.3.6",
    "prettier": "^2.0.4",
    "typescript": "^4.3.2"
  }
